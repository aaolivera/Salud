<div class="col-12" data-bind="with: Modificando">
    <div class="card text-white bg-info mb-3" style="width:100%">
        <div class="card-body">
            <form class="needs-validation" id="myForm" novalidate>
                <div class="form-row">
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Nombre</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Nombre" required data-bind="value: Nombre">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Apellido</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Apellido" required data-bind="value: Apellido">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Dni</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Dni" required data-bind="value: Dni">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Direccón</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Direccón" required data-bind="value: Direccion">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Tel</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Tel" required data-bind="value: Telefono">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-form-label col-form-label-sm">Telefono alternativo</label>
                        <input type="text" class="form-control form-control-sm" placeholder="TelefonoAlternativo" required data-bind="value: TelefonoAlternativo">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-4 mb-3">
                        <label class="col-form-label col-form-label-sm">Mail</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Mail" required data-bind="value: Mail">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4 mb-3">
                        <label class="col-form-label col-form-label-sm">Matricula</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Matricula" required data-bind="value: Matricula">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                    <div class="col-md-4 mb-3">
                        <label class="col-form-label col-form-label-sm">CBU</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Cbu" required data-bind="value: Cbu">
                        <div class="invalid-feedback">
                            El dato es obligatorio
                        </div>
                    </div>
                </div>
                <div class="alert alert-warning" role="alert" style="padding-top:0">
                    <div>Precios</div>
                    <div class="form-row">
                        <div class="col-md-3">
                            <label class="col-form-label col-form-label-sm">Kinesiologia</label>
                            <input type="text" class="form-control form-control-sm" placeholder="Cantidad" required data-bind="value: PrecioKinesiologia">
                            <div class="invalid-feedback">
                                El dato es obligatorio
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="col-form-label col-form-label-sm">Enfermeria</label>
                            <input type="text" class="form-control form-control-sm" placeholder="Cantidad" required data-bind="value: PrecioEnferemeria">
                            <div class="invalid-feedback">
                                El dato es obligatorio
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="col-form-label col-form-label-sm">Guardia</label>
                            <input type="text" class="form-control form-control-sm" placeholder="Cantidad" required data-bind="value: PrecioGuardia">
                            <div class="invalid-feedback">
                                El dato es obligatorio
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="col-form-label col-form-label-sm">Cuidador</label>
                            <input type="text" class="form-control form-control-sm" placeholder="Cantidad" required data-bind="value: PrecioCuidador">
                            <div class="invalid-feedback">
                                El dato es obligatorio
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row mx-auto">
                    <div class="col-md-2 offset-4">
                        <button class="btn btn-primary" style="width:100%" data-bind="click: $root.Models[1].Crear, visible: Id() == 0">Crear</button>
                        <button class="btn btn-primary" style="width:100%" data-bind="click: $root.Models[1].Modificar, visible: Id() != 0">Modificar</button>
                    </div>
                    <div class="col-md-2">
                        <button class="btn btn-primary" style="width:100%" data-bind="click: $root.Models[1].Cancelar">Cancelar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>